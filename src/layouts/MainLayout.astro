---
import MainHead from './MainHead.astro';

interface Props {
	title?: string;
	description?: string;
}

const {
	title = "Jason's Blog",
	description = "This is where I try to contemplate and put into words the things I've coded.",
} = Astro.props;
---

<html lang='en'>
	<MainHead {title} {description} />
	<body>
		<slot>Default Text</slot>
	</body>
</html>
